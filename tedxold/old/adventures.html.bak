      $(window).resize(adjust());

      function scrollto(id) {
        var etop = $("#" + id).offset().top;
        $("html, body").animate(
          {
            scrollTop: etop
          },
          1000
        );
      }
      $("#nominationform").one("submit", function() {
        var nominatorname = encodeURIComponent($("#nominatorname").val());
        var nominatoremail = encodeURIComponent($("#nominatoremail").val());
        var nomineename = encodeURIComponent($("#nomineename").val());
        var fieldofachievement = encodeURIComponent($("#profession").val());
        var nomineeemail = encodeURIComponent($("#nomineeemail").val());
        var nomineecontact = encodeURIComponent($("#nomineecontact").val());
        var baseURL =
          "https://docs.google.com/forms/d/e/1FAIpQLSc3eqtrV8AykiCgjuj0-IViWXN9gfdkqrds187b319hgqka1A/formResponse?entry.1349622509=";
        var submitURL =
          baseURL +
          nominatorname +
          "&entry.714106184=" +
          nominatoremail +
          "&entry.1799909845=" +
          nomineename +
          "&entry.1578682230=" +
          fieldofachievement +
          "&entry.2122985002=" +
          nomineeemail +
          "&entry.1365800405=" +
          nomineecontact +
          "&submit=submit";
        $(this)[0].action = submitURL;
        $("#successmessage")
          .css("visibility", "visible")
          .delay(800);
      });
